<Window x:Class="Учет_просмотренных_фильмов.browser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Учет_просмотренных_фильмов"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Постер" Height="450" Width="800" Background="#C7C0D6" WindowStyle="None" Style="{StaticResource WindowStyle}">
    <Grid>
        <WebBrowser x:Name="web" Margin="10,10,9.6,55.8"/>
        <Button x:Name="ok" Content="OK" Margin="10,376,680.6,9.8" RenderTransformOrigin="0.5,0.502" Click="Ok_Click"/>
        <Button x:Name="cancel" Content="Отмена" Margin="681,376,9.6,9.8" Click="Cancel_Click"/>
    </Grid>
</Window>
